struct Time {
  hour: i32,
  second: i32
}

struct Date {
  year: i32,
  month: i32,
  day: i32
}

struct DateTime {
  date: Date,
  time: Time 
}

func gib(): i32 {
  return 123;
}

func gib2(a: i32, b: i32, c: bool): i32 {
  if (c) {
    return a - b;
  }
  return a + b;
}

func main(): i32 {
  var date: Date = Date { year: 2024, month: gib2(5, 2, false), day: 15 };
  var year: i32 = date.year + date.month * 43 + date.day - (date.month + 45 / 4);
  return gib2(date.year * date.month, date.day - 32, date.day);
}
