func meh(a: i32, b: i32) : i32 {
  if (a < 3) {
    b = b + 2;
    return b * a;
  }
  return a + b;
}

func add2(a: i32, b: i32, add: bool) : i32 {
  if (add) {
    return a + b;
  }
  return a;
}

func main(): i32 {
  var a: i32 = 10;
  var b: i32 = 20;
  var c: i32 = add2(add2(1, 1, true), b, b >= a);
  return c;
}
